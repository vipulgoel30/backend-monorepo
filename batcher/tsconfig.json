{
  "compilerOptions": {
    "lib": ["ES2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es6",
    "esModuleInterop": true,
    "rootDir": "src",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "skipLibCheck": false,
    "strict": true,
    "typeRoots": ["./process-env.d.ts"],
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true
  },
  "exclude": []
}
